<?xml version="1.0" encoding="utf-8"?>
<plugin version="1.0">
	<title xml:lang="en">PlantUML Formatter</title>
	<title xml:lang="ko">PlantUML 포매터</title>
        <link>https://github.com/mete0r/textcube-plantuml</link>
	<version>0.0</version>
        <description xml:lang="en">Implements PlantUML(http://plantuml.sourceforge.net) formatter.</description>
        <description xml:lang="ko">PlantUML(http://plantuml.sourceforge.net) 문법을 구현합니다.</description>
	<license>Affero GPL v3</license>
        <author xml:lang="en" link="https://github.com/mete0r">mete0r</author>
        <author xml:lang="ko" link="https://github.com/mete0r">mete0r</author>
	<safety changeData="no" exposeData="no" accessLocal="no" accessRemote="no" accessRaw="no" />
	<requirements>
		<textcube>1.7</textcube>
	</requirements>
	<binding>
		<formatter id="plantuml" name="PlantUML">
			<format>FM_PlantUML_format</format>
			<summary>FM_PlantUML_summary</summary>
			<usedFor editor="plain" />
		</formatter>
	</binding>
</plugin>
